{
    "name": "stevegrunwell/wp-encrypted-options",
    "description": "API for saving encrypted data into the wp_options table",
    "type": "wordpress-plugin",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "wordpress-plugin",
        "wordpress-options",
        "encryption"
    ],
    "authors": [
        {
            "name": "Steve Grunwell",
            "email": "steve@stevegrunwell.com",
            "homepage": "https://stevegrunwell.com"
        }
    ],
    "support": {
        "issues": "https://github.com/stevegrunwell/wp-encrypted-options/issues",
        "source": "https://github.com/stevegrunwell/wp-encrypted-options"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "composer/installers": "^1.5",
        "defuse/php-encryption": "^2.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "wimg/php-compatibility": "^8.1",
        "wp-coding-standards/wpcs": "^0.14"
    },
    "autoload": {
      "classmap": [
        "includes/"
      ],
      "files": [
        "wp-encrypted-options.php"
      ]
    }
}
